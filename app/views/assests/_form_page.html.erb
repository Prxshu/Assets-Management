

<div class="container">
<%= render 'validation',obj: @assest%>
<%= form_with(model: @assest, data: {turbo:false})  do |f| %>
  
    <p>
      <%= f.label :Asset_name  %><br/>
      <%= f.text_field :assest_name %>
    </p>
  
    <p>
      <%= f.label :Serial_no %><br/>
      <%= f.text_field :serial_no %>
    </p>

    <p>
        <%= f.label :Category %><br/>
        <%= f.text_field :category %>
    </p>

    <p>
        <%= f.label :Asset_ammount %><br/>
        <%= f.text_field :aesset_ammount %>
    </p>

    <p>
        <%= f.label :department %><br/>
        <%= f.text_field :department %>
    </p>

    <p>
        <%= f.label :description %><br/>
        <%= f.text_area :description %>
    </p>
  
    <p>
      <%= f.submit %>
    </p>
  
<% end %>

</div>